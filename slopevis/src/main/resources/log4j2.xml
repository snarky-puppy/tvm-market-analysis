<Configuration packages="com.tvm" status="DEBUG">
    <Appenders>
        <GuiAppender name="GuiAppender">
            <PatternLayout pattern="%p: %msg%n"/>
        </GuiAppender>
    </Appenders>
    <Loggers><Root level="trace"><AppenderRef ref="GuiAppender"/></Root></Loggers>
</Configuration>

